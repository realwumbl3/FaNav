{
  "manifest_version": 3,
  "name": "FaNav. by wumbl3.",
  "version": "0.0.1",
  "description": "Enhancement suit for Furaffinity.",
  "content_scripts": [
    {
      "matches": [
        "*://www.furaffinity.net/*",
        "*://furaffinity.net/*"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "src/*"
      ]
    }
  ],
  "icons": {
    "128": "icon.128.png"
  }
}